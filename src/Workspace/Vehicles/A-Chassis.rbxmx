&#9;&#9;script.Parent.Toggle.TextColor3 = Color3.new(0,0,0)
&#9;&#9;window:TweenPosition(UDim2.new(.5, 0, .5, 0), d, s, t, true, nil)
&#9;&#9;open = true
&#9;end
end)


