&#9;&#9;&#9;-- Toggle PBrake
&#9;&#9;elseif input.KeyCode ==_CTRL[&quot;ContlrPBrake&quot;] or (_MSteer and input.KeyCode==_CTRL[&quot;MousePBrake&quot;]) or ((not _MSteer) and input.KeyCode==_CTRL[&quot;PBrake&quot;]) or ((not _MSteer) and input.KeyCode==_CTRL[&quot;PBrake2&quot;]) then
